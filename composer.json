{
    "name": "codicastudio/mixpanel",
    "description": "A noobs worthless garbage, just learning.",
    "keywords": [
        "worthless garabage by a noob."
    ],
    "homepage": "https://github.com/codicastudio/mixpanel",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael J Brown",
            "email": "codicastudiodev@gmail.com",
            "homepage": "https://codicastudio.dev",
            "role": "Developer"
        }
    ],

    "autoload": {
        "psr-4": {
            "codicastudio\\LaravelMixpanel\\": "src/"
        }
    },
    "require": {
        "illuminate/auth": "^7.0",
        "illuminate/config": "^7.0",
        "illuminate/console": "^7.0",
        "illuminate/events": "^7.0",
        "illuminate/http": "^7.0",
        "illuminate/routing": "^7.0",
        "illuminate/queue": "^7.0",
        "illuminate/support": "^7.0",
        "mixpanel/mixpanel-php": "^2.7",
        "sinergi/browser-detector": "^6.1"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.8",
        "codicastudio/masterbase": "^7.0",
        "laravel/browser-kit-testing": "^6.0",
        "phpmd/phpmd": "^2.7",
        "phpunit/phpunit": "^8.5",
        "sebastian/phpcpd": "^5.0",
        "symfony/thanks": "^1.2",
        "php-coveralls/php-coveralls": "^2.1",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload-dev": {
        "psr-4": {
            "codicastudio\\LaravelMixpanel\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "codicastudio\\LaravelMixpanel\\Providers\\Service"
            ],
            "alias": {
                "Mixpanel": "codicastudio\\LaravelMixpanel\\Facades\\Mixpanel"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
